<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://cdm.ladbrokes.co.uk/banking/Payment/2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://cdm.ladbrokes.co.uk/banking/Payment/2.0" xmlns:pay="http://cdm.ladbrokes.co.uk/banking/Payment/2.0" xmlns:man="http://cdm.ladbrokes.co.uk/common/ManagedEntity/2.0" xmlns:ptr="http://cdm.ladbrokes.co.uk/party/PartyRole/2.0" xmlns:txn="http://cdm.ladbrokes.co.uk/customer/Transaction/2.0" xmlns:chn="http://cdm.ladbrokes.co.uk/common/Channel/2.0" xmlns:cty="http://cdm.ladbrokes.co.uk/common/Country/2.0" xmlns:loc="http://cdm.ladbrokes.co.uk/common/Location/2.0" xmlns:xref="http://cdm.ladbrokes.co.uk/common/CrossRef/2.0" xmlns:imy="http://cdm.ladbrokes.co.uk/common/InternationalMoney/2.0" xmlns:typ="http://cdm.ladbrokes.co.uk/common/BaseType/2.0" xmlns:cus="http://cdm.ladbrokes.co.uk/customer/Customer/2.0">
	<xs:import namespace="http://cdm.ladbrokes.co.uk/customer/Customer/2.0" schemaLocation="../../Customer/2.0/Customer.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/BaseType/2.0" schemaLocation="../../BaseType/2.0/BaseType.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/InternationalMoney/2.0" schemaLocation="../../InternationalMoney/2.0/InternationalMoney.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/CrossRef/2.0" schemaLocation="../../CrossRef/2.0/CrossRef.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/Location/2.0" schemaLocation="../../Location/2.0/Location.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/Country/2.0" schemaLocation="../../Country/2.0/Country.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/Channel/2.0" schemaLocation="../../Channel/2.0/Channel.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/customer/Transaction/2.0" schemaLocation="../../Transaction/2.0/Transaction.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/party/PartyRole/2.0" schemaLocation="../../PartyRole/2.0/PartyRole.xsd"/>
	<xs:import namespace="http://cdm.ladbrokes.co.uk/common/ManagedEntity/2.0" schemaLocation="../../ManagedEntity/2.0/ManagedEntity.xsd"/>
	<xs:element name="PaymentMethodCharacteristic" type="PaymentMethodCharacteristic"/>
	<xs:complexType name="PaymentMethodCharacteristic">
		<xs:annotation>
			<xs:documentation>A generic container of name value pair characteristics for future payment extensibility</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The name of the generic characteristic</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The value of the generic characteristic</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valueType" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The type of the value of the generic characteristic (if applicable)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>Active or Inactive</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Paybox" type="Paybox"/>
	<xs:complexType name="Paybox">
		<xs:annotation>
			<xs:documentation>The Paybox payment method</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="payboxNumber" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Paybox account number</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="WesternUnion" type="WesternUnion"/>
	<xs:complexType name="WesternUnion">
		<xs:annotation>
			<xs:documentation>This class represents the western union details of a customer used to transfer payment via western union</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="payee" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>A name that the payee that of this western union account</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="accountNumber" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The western union account number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="office" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The office name of the western union branch where this payment method is registered</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="branchCode" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The western union branch code</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="branchCountry" type="cty:Country" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataCash" type="DataCash"/>
	<xs:complexType name="DataCash">
		<xs:annotation>
			<xs:documentation>The datacash payment method</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="dataCashUserId" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The datacash user Id</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="subType" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The subtype of datacash card.
e.g: PaySafeCard, Sofort</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CreditPayment" type="CreditPayment"/>
	<xs:complexType name="CreditPayment">
		<xs:annotation>
			<xs:documentation>This would result in transferring money INTO an account FROM Ladbrokes.

e.g. The result of a 'WithdrawFunds' initiated by a Customer, or Paying an Affiliate</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Payment">
				<xs:sequence>
					<xs:element name="creditNote" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>A note on the credit payemnt</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Payment" type="Payment"/>
	<xs:complexType name="Payment" abstract="true">
		<xs:annotation>
			<xs:documentation>This class represents a payment with a payment provider.  A Wallet Transaction (process) may or may not have a payment associated with it (e.g:  DepositFunds transaction does, TransferFunds does not).

A Payment may also originate that is not intended for customers, rather employees.

Thus Wallet Transaction and Payment are separate concepts and modelled as such.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="txn:Transaction">
				<xs:sequence>
					<xs:element name="paymentKey" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>A Unique payment Key identifying this payment</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
					<xs:element name="paymentSettledDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
					<xs:element name="paymentSort" type="PaymentSortKind" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Sort of payment: 'W'ithdraw or 'D'eposit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="amount" type="imy:InternationalMoney" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The amount</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentChannel" type="chn:Channel" minOccurs="0" maxOccurs="1"/>
					<xs:element name="paymentCountry" type="cty:Country" minOccurs="0" maxOccurs="1"/>
					<xs:element name="paymentMethod" type="PaymentMethod" minOccurs="0" maxOccurs="1"/>
					<xs:element name="paymentCharacteristics" type="PaymentCharacteristics" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Ukash" type="Ukash"/>
	<xs:complexType name="Ukash">
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="ukashExternalId" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The ukash account id</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The email address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isVerified" type="xs:boolean" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Is this verified?</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Neteller" type="Neteller"/>
	<xs:complexType name="Neteller">
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="netellerId" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The Neteller user ID</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Cheque" type="Cheque"/>
	<xs:complexType name="Cheque">
		<xs:annotation>
			<xs:documentation>Cheque payment method</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="payee" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The payee the cheque is to be made out to</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressLine1" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The address line 1 of the payee address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressLine2" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The address line 2 of the payee address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressLine3" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The address line 3 of the payee address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressLine4" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The address line 4 of the payee address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressPostcode" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The postcode of the payee address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressCity" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The city of the payee address</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="address" type="loc:Address" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Card" type="Card"/>
	<xs:complexType name="Card">
		<xs:annotation>
			<xs:documentation>A Card type of payment method</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="cardNo" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>User's card number (Only the first 6 and last 4 digits of the card number are returned).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cardKind" type="CardSortKind" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The kind of the card, Credit or Debit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="startDate" type="xs:date" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>User's payment method start date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="endDate" type="xs:date" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>User's payment method end date.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="issueNo" type="xs:integer" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>User's payment method issue number (note some cards do not have a issue number).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="country" type="cty:CountryCodeKind" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Country the cardholder is from</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cardHolderName" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Name on the card</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cardScheme" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Scheme for the card.
e.g:  VISA, VISA Delta, Mastercard, Switch</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cardStatus" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Specific card status (e.g. Active, Suspended (fraud), Expired, etc)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cvv" type="xs:integer" minOccurs="0" maxOccurs="1" />
					<xs:element name="isHidden" type="xs:boolean" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Is this card hidden?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cardToken" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Payment gateway encypted token for every card stored</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isPrimaryCard" type="xs:boolean" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Indicates if the card is primary among all cards registered</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="PaymentSortKind">
		<xs:annotation>
			<xs:documentation>Sorts of Payment available to the customer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Debit">
				<xs:annotation>
					<xs:documentation>Debit customer's account aka Deposit</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Credit">
				<xs:annotation>
					<xs:documentation>Credit customer's account aka Withdrawal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Paypal" type="Paypal"/>
	<xs:complexType name="Paypal">
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="userName" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>Paypal user name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The email of the paypal account</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="paypalCountry" type="cty:Country" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="PaymentMethodStatusKind">
		<xs:annotation>
			<xs:documentation>Enumeration of the status values for a Payment Method.

Each payment method may have further specialisations of status related to the type (e.g. suspended for fraud - for a card)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Active">
				<xs:annotation>
					<xs:documentation>The payment method is Active</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Inactive">
				<xs:annotation>
					<xs:documentation>The payment method is Active</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other">
				<xs:annotation>
					<xs:documentation>TBD - Placeholder</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ClickAndBuy" type="ClickAndBuy"/>
	<xs:complexType name="ClickAndBuy">
		<xs:annotation>
			<xs:documentation>The click and buy payment method</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
					<xs:element name="userName" type="xs:string" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>The username</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="createDateTime" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="PaymentMethodType" type="PaymentMethodType"/>
	<xs:complexType name="PaymentMethodType">
		<xs:annotation>
			<xs:documentation>The type of the payment method.  e.g. Card, Paypal.

Each type will have Ladbrokes defined constraints on aspects such as withdrawal and deposit limits</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code" type="PaymentMethodTypeKind" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The code representing the payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The name of the payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>A description</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumDeposit" type="typ:Money" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The minimum deposit amount for this payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumDeposit" type="typ:Money" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The maximum deposit amount for this payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="depositProcessingTime" type="xs:integer" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The deposit processing time In hours - 0 is 'Instant'</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="withdrawalProcessingTime" type="xs:integer" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>In hours - 0 is 'Instant'</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="depositFee" type="typ:Percentage" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>A fee for depositing if applicable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="withdrawalFee" type="typ:Percentage" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>A fee for withdrawing if applicable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="xs:string" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>A notes on the method</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="riskValue" type="xs:integer" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>Risk value for thjs payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentMethodTypeKeys" type="xref:CrossRefEntityKeys" minOccurs="0" maxOccurs="1"/>
			<xs:element name="minimumWithdrawal" type="typ:Money" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The minimum withdrawal amount for this payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumWithdrawal" type="typ:Money" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The maximum withdrawal amount for this payment method type</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PaymentCharacteristics" type="PaymentCharacteristics"/>
	<xs:complexType name="PaymentCharacteristics">
		<xs:annotation>
			<xs:documentation>This class is a collection of payment characteristics</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="numChars" type="xs:integer" minOccurs="0" maxOccurs="1" >
				<xs:annotation>
					<xs:documentation>The number of characteristics in the collection</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentCharacteristic" type="PaymentCharacteristic" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BetCard" type="BetCard"/>
	<xs:complexType name="BetCard">
		<xs:complexContent>
			<xs:extension base="PaymentMethod">
				<xs:sequence>
